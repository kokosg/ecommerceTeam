<HTML>
<head>
  <meta charset="ISO-8859-1">
  #include( "/commonElements/head.html" )
  <title>$apptitle</title>
</head>
#parse( "/commonElements/header.vm" )
<body>
<script src="assets/js/advanceSearch.js" type="text/javascript"></script>
<script src="assets/js/onLoad.js" type="text/javascript"></script>

<div class="jumbotron"></div>

<div class="bs-docs-section">
	<div class="row">
		<div class="col-lg-7 col-md-10 col-md-push-3">
			<!-- Form================================================== -->		

			<form action="subscribe" id="subscribe" method="post" class="form-horizontal">
			<fieldset>
			<input id="flag" name="flag" type="hidden" value = "1">
			<!-- Form Name -->
			<legend>Subscribe</legend>
			
			<!-- Text input-->
			<div class="control-group">
			  <label class="control-label" for="email">Email:</label>
			  <div class="controls">
			    <input id="email" name="email" type="text" placeholder="email@example.com" class="input-xlarge" required="">
			    <p class="help-block">in the format: email@example.com</p>
			  </div>
			</div>
			
			<!-- Button Drop Down -->
			<div class="control-group">
			  <label class="control-label" for="buttondropdown">Select Keyword:</label>
			  <div class="controls">
			    <div class="input-append">
			      <input id="buttondropdown" name="buttondropdown" class="input-xlarge" placeholder="select keyword" type="text">
			      <div class="btn-group">
			        <button class="btn dropdown-toggle" data-toggle="dropdown">
			          Keyword
			          <span class="caret"></span>
			        </button>
			        <ul class="dropdown-menu">
			          <li><a href="#">Option one</a></li>
			          <li><a href="#">Option two</a></li>
			          <li><a href="#">Option three</a></li>
			        </ul>
			      </div>
			    </div>
			  </div>
			</div>
			
			<!-- Multiple Checkboxes (inline) -->
			<div class="control-group">
			  <label class="control-label" for="editionSubscriber">Future Editions:</label>
			  <div class="controls">
			    <label class="checkbox inline" for="editionSubscriber-0">
			      <input type="checkbox" name="editionSubscriber" id="editionSubscriber-0" value="subscribe for future editions">
			      subscribe for future editions
			    </label>
			  </div>
			</div>
			
			<!-- Button -->
			<div class="control-group">
			  <label class="control-label" for="subscribe"></label>
			  <div class="controls">
			    <button id="subscribe" name="subscribe" class="btn btn-primary">Subscribe</button>
			    <input type="reset" class="btn btn-default" value="Reset" />
			  </div>
			</div>
			
			</fieldset>
			</form>

			

			</div>
		</div>
	</div>
</body>
</html>
</body>
#parse("/commonElements/footer.vm" )
</html>